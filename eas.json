{
  "cli": {
    "version": ">= 3.14.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ORS_API_KEY": "coloque_aqui_sua_chave_ors",
        "FIREBASE_API_KEY": "coloque_aqui_sua_chave_firebase",
        "FIREBASE_AUTH_DOMAIN": "coloque_aqui_seu_auth_domain",
        "FIREBASE_DB_URL": "coloque_aqui_sua_url_do_banco",
        "FIREBASE_PROJECT_ID": "coloque_aqui_o_id_do_projeto",
        "FIREBASE_STORAGE_BUCKET": "coloque_aqui_seu_storage_bucket",
        "FIREBASE_MESSAGING_SENDER_ID": "coloque_aqui_seu_sender_id",
        "FIREBASE_APP_ID": "coloque_aqui_seu_app_id"
      }
    }
  }
}